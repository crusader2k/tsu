{
  "rulesDirectory": [
    // Higher up means higher priority (in case of duplicates)
    "./node_modules/tslint-consistent-codestyle/rules",
    "./node_modules/tslint-eslint-rules/dist/rules",
    "./node_modules/tslint-microsoft-contrib"
  ],
  "rules": {
    "adjacent-overload-signatures": true,
    "arrow-parens": [true, "ban-single-arg-parens"],
    "arrow-return-shorthand": true,
    "ban-comma-operator": true,
    "ban-ts-ignore": true,
    "callable-types": true,
    "class-name": true,
    "comment-format": [true, "check-space"],
    "curly": true,
    "cyclomatic-complexity": [true, 10],
    "encoding": true,
    "eofline": true,
    "forin": true,
    "function-constructor": true,
    "import-spacing": true,
    "increment-decrement": true,
    "indent": [true, "tabs", 4],
    "interface-name": [true, "never-prefix"],
    "interface-over-type-literal": true,
    "label-position": true,
    "linebreak-style": [true, "CRLF"],
    "member-access": true,
    "new-parens": true,
    "no-angle-bracket-type-assertion": true,
    "no-arg": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": [true, 1],
    "no-construct": true,
    "no-debugger": true,
    "no-default-import": true,
    "no-duplicate-imports": true,
    "no-duplicate-super": true,
    "no-duplicate-switch-case": true,
    "no-duplicate-variable": true,
    "no-dynamic-delete": true,
    "no-empty-interface": true,
    "no-empty": [true, "allow-empty-catch"],
    "no-eval": true,
    "no-implicit-dependencies": [true, "dev"],
    "no-inferrable-types": true,
    "no-internal-module": true,
    "no-invalid-template-strings": true,
    "no-invalid-this": true,
    "no-irregular-whitespace": true,
    "no-mergeable-namespace": true,
    "no-misused-new": true,
    "no-namespace": [true, "allow-declarations"],
    "no-object-literal-type-assertion": true,
    "no-redundant-jsdoc": true,
    "no-reference-import": true,
    "no-reference": true,
    "no-return-await": true,
    "no-shadowed-variable": true,
    "no-sparse-arrays": true,
    "no-string-literal": true,
    "no-string-throw": true,
    "no-switch-case-fall-through": true,
    "no-this-assignment": [true, { "allow-destructuring": true }],
    "no-trailing-whitespace": [true, "ignore-template-strings"],
    "no-unnecessary-class": true,
    "no-unsafe-finally": true,
    "no-unused-expression": true,
    "no-var-keyword": true,
    "no-var-requires": true,
    "number-literal-format": true,
    "object-literal-key-quotes": [true, "as-needed"],
    "object-literal-shorthand": [true, "never"],
    "one-line": [true, "check-open-brace", "check-whitespace"],
    "one-variable-per-declaration": true,
    "only-arrow-functions": [true, "allow-declarations"],
    "prefer-conditional-expression": [true, "check-else-if"],
    "prefer-const": true,
    "prefer-for-of": true,
    "prefer-method-signature": true,
    "prefer-object-spread": true,
    "prefer-while": true,
    "quotemark": [true, "single", "jsx-double"],
    "radix": true,
    "semicolon": [true, "always"],
    "space-within-parens": [true, 0],
    "switch-final-break": [true, "always"],
    "triple-equals": [true, "allow-undefined-check"],
    "type-literal-delimiter": true,
    "unified-signatures": true,
    "unnecessary-constructor": true,
    "use-isnan": true,
    "variable-name": [true, "ban-keywords"],
    // Enabled (tslint)
    "align": [true,
      "parameters",
      "members",
      "statements",
      "elements"
    ],
    "comment-type": [true,
      "singleline",
      "doc",
      "directive"
    ],
    "member-ordering": [true, {
      "order": [
        "static-field",
        "instance-field",
        "constructor",
        "static-method",
        "instance-method"
      ]
    }],
    "space-before-function-paren": [true, {
      "anonymous": "always",
      "named": "never",
      "asyncArrow": "always",
      "method": "never",
      "constructor": "never"
    }],
    "trailing-comma": [true, {
      "multiline": "never",
      "singleline": "never"
    }],
    "typedef-whitespace": [true, {
      "call-signature": "nospace",
      "index-signature": "nospace",
      "parameter": "nospace",
      "property-declaration": "nospace",
      "variable-declaration": "nospace"
    }],
    "whitespace": [true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-separator",
      "check-type"
    ],

    // Disabled (tslint)
    "array-type": false,
    "ban-types": false,
    "ban": false,
    "binary-expression-operand-order": false,
    "completed-docs": false,
    "file-header": false,
    "file-name-casing": false,
    "import-blacklist": false,
    "jsdoc-format": false,
    "max-classes-per-file": false,
    "max-file-line-count": false,
    "max-line-length": false,
    "newline-before-return": false,
    "newline-per-chained-call": false,
    "no-any": false,
    "no-bitwise": false,
    "no-console": false,
    "no-default-export": false,
    "no-import-side-effect": false,
    "no-magic-numbers": false,
    "no-non-null-assertion": false,
    "no-null-keyword": false,
    "no-parameter-properties": false,
    "no-parameter-reassignment": false,
    "no-require-imports": false,
    "no-submodule-imports": false,
    "no-unnecessary-callback-wrapper": false,
    "no-unnecessary-initializer": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "prefer-function-over-method": false,
    "prefer-switch": false,
    "prefer-template": false,
    "switch-default": false,
    "typedef": false,
    "typeof-compare": false,
    "unnecessary-bind": false,

    // Requires type-checking (tslint)
    "await-promise": false,
    "deprecation": false,
    "match-default-export-name": false,
    "no-boolean-literal-compare": false,
    "no-floating-promises": false,
    "no-for-in-array": false,
    "no-inferred-empty-object-type": false,
    "no-unbound-method": false,
    "no-unnecessary-qualifier": false,
    "no-unnecessary-type-assertion": false,
    "no-unsafe-any": false,
    "no-unused-variable": false,
    "no-use-before-declare": false,
    "no-void-expression": false,
    "prefer-readonly": false,
    "promise-function-async": false,
    "restrict-plus-operands": false,
    "return-undefined": false,
    "strict-boolean-expressions": false,
    "strict-type-predicates": false,
    "use-default-type-parameter": false,

    // Enabled (tslint-consistent-codestyle)
    "no-accessor-recursion": true,
    "no-collapsible-if": true,
    "no-var-before-return": true,
    "parameter-properties": [true, "all-or-none"],
    "prefer-const-enum": true,
    "no-unused": [true,
      "unused-function-expression-name",
      "unused-class-expression-name",
      "unused-catch-binding"
    ],
    "naming-convention": [true,
      {"type": "default", "format": "camelCase", "leadingUnderscore": "forbid", "trailingUnderscore": "forbid"},
      {"type": "variable", "modifiers": "unused", "leadingUnderscore": "allow"},
      {"type": "variable", "modifiers": "global", "leadingUnderscore": "allow"},
      {"type": "variable", "modifiers": ["global", "const"], "format": ["camelCase", "UPPER_CASE"]},
      {"type": "function", "leadingUnderscore": "forbid"},
      {"type": "parameter", "modifiers": "unused", "leadingUnderscore": "allow"},
      {"type": "member", "modifiers": "private", "leadingUnderscore": "require"},
      {"type": "member", "modifiers": "protected", "leadingUnderscore": "require"},
      {"type": "property", "modifiers": ["static", "const"], "format": ["camelCase", "UPPER_CASE"]},
      {"type": "type", "format": "PascalCase"},
      {"type": "enumMember", "format": ["PascalCase", "UPPER_CASE"]}
    ],

    // Disabled (tslint-consistent-codestyle)
    "const-parameters": false,
    "early-exit": false,
    "ext-curly": false,
    "no-as-type-assertion": false,
    "no-else-after-return": false,
    "no-return-undefined": false,
    "no-static-this": false,
    "no-unnecessary-else": false,
    "no-unnecessary-type-annotation": false,
    "object-shorthand-properties-first": false,
    "oddness-check": false,

    // Enabled (tslint-eslint-rules)
    "brace-style": [true, "stroustrup", { "allowSingleLine": true }],
    "no-constant-condition": true,
    "no-control-regex": true,
    "no-empty-character-class": true,
    "no-ex-assign": true,
    "no-extra-boolean-cast": true,
    "no-inner-declarations": true,
    "no-invalid-regexp": true,
    "no-regex-spaces": true,
    "ter-no-proto": true,
    "ter-no-script-url": true,
    "ter-no-self-compare": true,
    "ter-padded-blocks": [true, "never"],

    // Disabled (tslint-eslint-rules)
    "array-bracket-spacing": false,
    "block-spacing": false,
    "handle-callback-err": false,
    "no-duplicate-case": false,
    "no-extra-semi": false,
    "no-multi-spaces": false,
    "no-unexpected-multiline": false,
    "object-curly-spacing": false,
    "sort-imports": false,
    "space-in-parens": false,
    "ter-arrow-body-style": false,
    "ter-arrow-parens": false,
    "ter-arrow-spacing": false,
    "ter-computed-property-spacing": false,
    "ter-func-call-spacing": false,
    "ter-indent": false,
    "ter-max-len": [true, { "code": 80, "tabWidth": 4 }],
    "ter-newline-after-var": false,
    "ter-no-irregular-whitespace": false,
    "ter-no-mixed-spaces-and-tabs": false,
    "ter-no-sparse-arrays": false,
    "ter-no-tabs": false,
    "ter-prefer-arrow-callback": false,
    "valid-jsdoc": false,
    "valid-typeof": false,

    // Enabled  (tslint-microsoft-contrib)
    "mocha-avoid-only": true,
    "mocha-no-side-effect-code": true,
    "mocha-unneeded-done": true,
    "no-for-in": true,
    "no-function-constructor-with-string-args": true,
    "no-typeof-undefined": true,
    "no-unnecessary-bind": true,
    "no-unnecessary-override": true,
    "no-useless-files": true,
    "promise-must-complete": true,
    "use-named-parameter": true,

    // Disabled (tslint-microsoft-contrib)
    "chai-prefer-contains-to-index-of": false,
    "chai-vague-errors": false,
    "export-name": false,
    "function-name": false,
    "import-name": false,
    "informative-docs": false,
    "insecure-random": false,
    "jquery-deferred-must-complete": false,
    "max-func-body-length": false,
    "missing-jsdoc": false,
    "no-backbone-get-set-outside-model": false,
    "no-banned-terms": false,
    "no-cookies": false,
    "no-delete-expression": false,
    "no-disable-auto-sanitization": false,
    "no-document-domain": false,
    "no-document-write": false,
    "no-empty-line-after-opening-brace": false,
    "no-exec-script": false,
    "no-function-expression": false,
    "no-http-string": false,
    "no-increment-decrement": false,
    "no-inner-html": false,
    "no-jquery-raw-elements": false,
    "no-multiline-string": false,
    "no-octal-literal": false,
    "no-relative-imports": false,
    "no-reserved-keywords": false,
    "no-single-line-block-comment": false,
    "no-string-based-set-immediate": false,
    "no-string-based-set-interval": false,
    "no-string-based-set-timeout": false,
    "no-suspicious-comment": false,
    "no-unexternalized-strings": false,
    "no-unnecessary-field-initialization": false,
    "no-unnecessary-local-variable": false,
    "no-unnecessary-semicolons": false,
    "no-unsupported-browser-code": false,
    "no-with-statement": false,
    "non-literal-fs-path": false,
    "non-literal-require": false,
    "possible-timing-attack": false,
    "prefer-array-literal": false,
    "prefer-type-cast": false,
    "react-a11y-anchors": false,
    "react-a11y-aria-unsupported-elements": false,
    "react-a11y-event-has-role": false,
    "react-a11y-image-button-has-alt": false,
    "react-a11y-img-has-alt": false,
    "react-a11y-input-elements": false,
    "react-a11y-lang": false,
    "react-a11y-meta": false,
    "react-a11y-no-onchange": false,
    "react-a11y-props": false,
    "react-a11y-proptypes": false,
    "react-a11y-required": false,
    "react-a11y-role-has-required-aria-props": false,
    "react-a11y-role-supports-aria-props": false,
    "react-a11y-role": false,
    "react-a11y-tabindex-no-positive": false,
    "react-a11y-titles": false,
    "react-anchor-blank-noopener": false,
    "react-iframe-missing-sandbox": false,
    "react-no-dangerous-html": false,
    "react-this-binding-issue": false,
    "react-tsx-curly-spacing": false,
    "react-unused-props-and-state": false,
    "underscore-consistent-invocation": false,
    "use-simple-attributes": false,

    // Deprecated (tslint-microsoft-contrib)
    "missing-optional-annotation": false,
    "no-duplicate-parameter-names": false,
    "no-empty-interfaces": false,
    "no-missing-visibility-modifiers": false,
    "no-multiple-var-decl": false,
    "no-stateless-class": false,
    "no-var-self": false

    // Duplicates (tslint-microsoft-contrib)
    // "no-constant-condition": false,
    // "no-control-regex": false,
    // "no-duplicate-case": false,
    // "no-invalid-regexp": false,
    // "no-regex-spaces": false,
    // "valid-typeof": false,
  }
}
